    [  strPart(b,1,3)    ,   "hat"   ,   "strPart 1"],
    [  strPart(b,2,2)    ,   "at"    ,   "strPart 2"],
    [  strPart(a,1,3)    ,   "his"   ,   "strPart 3"],
  ];
