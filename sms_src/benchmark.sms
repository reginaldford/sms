//Use this to benchmark something

let first_time=[];
let new_time=[];
let time_elapsed=0;

benchmark=(expr)=>(
  print("Benchmarking: ");
  println(to_string(expr));
  first_time = time();
  eval(expr);
  new_time = time();
  println("Total time:");
  time_elapsed = (new_time[0]-first_time[0])*1000+0.001*(new_time[1]-first_time[1]);
  print(to_string(time_elapsed));
  println(" milliseconds")
);

fib = (n) => if( n < 2 , 1 ,fib(n - 1) + fib(n - 2));
benchmark(:fib(25));
