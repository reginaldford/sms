#Use this to benchmark something

let first_time=[];
let new_time=[];
let time_elapsed=0;

benchmark = (expr) =>   (
  put("Benchmarking: " str+ toStr(expr) str+ "\n");
  first_time = time();
  eval(expr);
  new_time = time();
  put("Total time:\n");
  time_elapsed = 1000 * (new_time[0] - first_time[0]) +  0.001 * (new_time[1] - first_time[1]);
  put(toStr(time_elapsed) str+ " milliseconds\n")
);

fib = (n) => if( n < 2 , 1 ,fib(n - 1) + fib(n - 2));
benchmark(:fib(25));
