#!/usr/local/bin/sms -qi
{
  let fib = n => {_gc();if( n < 2 ) 1 else fib(n - 1) + fib(n - 2);};
  let playFib = () => {
    while(true){
      put("Give me a number to fib. -1 to stop: ");
      let x = input();
      if(not(x=="-1"))
        putLn(str(fib(parse(x))))
      else return :done;
    };
  };
  playFib();
  cxImport(self,_scratch);
};
