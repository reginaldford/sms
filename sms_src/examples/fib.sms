#!/usr/local/bin/sms -qi
{
  let fib = n => if( n < 2 ) 1 else fib(n - 1) + fib(n - 2);
  let playFib = () => {
    while(true){
      put("Give me a number to fib. -1 to stop: ");
      let x = input();
      let parsedX = parse(x);
      if(not(parsedX==-1))
        putLn(str(fib(parsedX)))
      else return :done;
    };
  };
  playFib();
  cxImport(self,_scratch);
};
